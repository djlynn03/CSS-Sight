{
  "publisher": "djlynn03",
  "name": "css-sight",
  "displayName": "css-sight",
  "description": "Remove unused CSS rulesets",
  "repository": {
    "type": "git",
    "url": "https://github.com/djlynn03/CSS-Sight"
  },
  "license": "MIT",
  "icon": "icon.png",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Language Packs",
    "Linters",
    "Formatters"
  ],
  "keywords": [
    "css",
    "css-sight",
    "lint",
    "class",
    "unused",
    "clean",
    "format",
    "formatter"
  ],
  "contributes": {
    "commands": [
      {
        "command": "css-sight.removeAllUnusedSelectorsInFile",
        "title": "CSS Sight: Remove All Unused Selectors in File"
      },
      {
        "command": "css-sight.removeAllUnusedSelectorsInWorkspace",
        "title": "CSS Sight: Remove All Unused Selectors in Workspace"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:css",
    "onLanguage:html",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "copy-test-workspace": "cp -r src/test/test-workspace out/test/test-workspace",
    "copy-test-workspace-windows": "xcopy src\\test\\test-workspace out\\test\\test-workspace /s /i /q /y",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "npm run copy-test-workspace && vscode-test",
    "test-windows": "npm run copy-test-workspace-windows && vscode-test",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/mocha": "^10.0.10",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.13.4",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.2",
    "eslint": "^9.16.0",
    "mock-fs": "^5.5.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/parser": "^7.26.9",
    "@babel/traverse": "^7.26.9",
    "cheerio": "^1.0.0",
    "glob": "^7.1.1",
    "postcss": "^8.5.1",
    "postcss-selector-parser": "^7.0.0",
    "sinon": "^19.0.2"
  }
}
